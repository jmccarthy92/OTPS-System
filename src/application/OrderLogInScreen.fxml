<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="140.0" prefWidth="341.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.OrderLoginController">
   <center>
      <Pane prefHeight="140.0" prefWidth="312.0" styleClass="background" stylesheets="@OTPSTheme.css" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="userField" layoutX="81.0" layoutY="27.0" promptText="Enter UserName..." />
            <Button fx:id="loginButton" layoutX="238.0" layoutY="50.0" mnemonicParsing="false" onAction="#loginButtonClick" styleClass="button-viewHist" stylesheets="@OTPSTheme.css" text="Log-In" />
            <Button fx:id="backButton" layoutX="8.0" layoutY="95.0" mnemonicParsing="false" onAction="#backButtonClick" styleClass="button-viewHist" stylesheets="@OTPSTheme.css" text="Back" />
            <PasswordField fx:id="pwField" layoutX="81.0" layoutY="63.0" promptText="Enter Password..." />
         </children>
      </Pane>
   </center>
</BorderPane>
